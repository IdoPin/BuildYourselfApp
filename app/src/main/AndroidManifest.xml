<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.buildyourself">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_img"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_img_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar">
        <activity
            android:name="com.example.buildyourselfapp.Activities.Activity_Add_Image"
            android:exported="false" />
        <activity
            android:name="com.example.buildyourselfapp.Activities.Activity_Register"
            android:exported="false" />
        <activity
            android:name="com.example.buildyourselfapp.Activities.Activity_Plan"
            android:exported="false" />
        <activity
            android:name="com.example.buildyourselfapp.Activities.Activity_Meals"
            android:exported="false" />
        <activity
            android:name="com.example.buildyourselfapp.Activities.Activity_Exercise"
            android:exported="false" />
        <activity
            android:name="com.example.buildyourselfapp.Activities.Activity_template"
            android:exported="false" />
        <activity
            android:name="com.example.buildyourselfapp.Activities.Activity_Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>